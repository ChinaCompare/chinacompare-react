{
  "name": "web-react",
  "version": "0.2.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ChinaCompare/web-react"
  },
  "main": "build/server/index.js",
  "scripts": {
    "preinstall": "node tools/scripts/preinstall",
    "prepare": "yarn && yarn semantic && yarn intl",
    "clean": "babel-node tools/scripts/clean",
    "development": "yarn flow:defs && NODE_ENV=development babel-node tools/development",
    "dev": "yarn development",
    "build": "yarn prepare && cross-env NODE_ENV=production babel-node tools/scripts/build",
    "analyze": "babel-node tools/scripts/analyze",
    "start": "yarn build && cross-env NODE_ENV=production node build/server",
    "deploy": "babel-node tools/scripts/deploy",
    "flow": "babel-node tools/scripts/flow",
    "flow:defs": "flow-typed install --overwrite",
    "flow:coverage": "flow-coverage-report -i 'src/shared/**/*.js' -t html -t json -t text ; rimraf flow-coverage",
    "lint": "eslint src config tools",
    "audit": "nsp check package",
    "validate": "yarn audit && yarn lint",
    "coverage": "yarn test -- --coverage && cat coverage/lcov.info | coveralls ; rimraf coverage",
    "test": "node tools/scripts/tester.js --env=jsdom",
    "fixsass": "node node_modules/node-sass/scripts/install.js && npm rebuild node-sass",
    "semantic": "cd semantic && yarn && gulp build",
    "intl": "yarn intl:extractLocaleData && yarn intl:extractMessages",
    "intl:extractLocaleData": "babel-node tools/scripts/extractLocaleData",
    "intl:extractMessages": "babel-node tools/scripts/extractMessages"
  },
  "license": "MIT",
  "jest": {
    "collectCoverageFrom": [
      "src/shared/**/*.{js,jsx}"
    ],
    "coverageDirectory": "./coverage",
    "testPathIgnorePatterns": [
      "<rootDir>/(build|tools|node_modules|flow-typed|public)/"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/tools/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/tools/jest/fileTransform.js"
    }
  },
  "dependencies": {
    "app-root-dir": "1.0.2",
    "axios": "0.15.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bluebird": "^3.4.7",
    "code-split-component": "2.0.0-alpha.5",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cookie-parser": "^1.4.3",
    "dotenv": "4.0.0",
    "express": "4.14.0",
    "express-request-language": "^1.1.9",
    "helmet": "3.4.0",
    "hpp": "0.2.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "modernizr": "3.3.1",
    "normalize.css": "5.0.0",
    "offline-plugin": "4.5.5",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-helmet": "4.0.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-intl": "^2.2.3",
    "react-jobs": "0.6.2",
    "react-redux": "5.0.2",
    "react-router": "4.0.0-alpha.6",
    "react-router-addons-routes": "^0.0.3",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "reselect": "^2.5.4",
    "semantic-ui-css": "^2.2.4",
    "semantic-ui-react": "^0.64.3",
    "serialize-javascript": "1.3.0",
    "user-home": "2.0.0",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.5.1",
    "babel-cli": "6.22.2",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "18.0.0",
    "babel-loader": "6.2.10",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-react-intl": "^2.3.0",
    "babel-plugin-transform-react-jsx-self": "6.22.0",
    "babel-plugin-transform-react-jsx-source": "6.22.0",
    "babel-polyfill": "6.22.0",
    "babel-preset-env": "1.1.8",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.22.0",
    "babel-preset-stage-3": "6.22.0",
    "babel-template": "6.22.0",
    "chokidar": "1.6.1",
    "coveralls": "^2.11.15",
    "cross-env": "3.1.4",
    "css-loader": "0.26.1",
    "deep-freeze-strict": "^1.1.1",
    "deepmerge": "^1.3.1",
    "eslint": "3.14.0",
    "eslint-config-airbnb": "14.0.0",
    "eslint-plugin-flowtype": "2.30.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "3.0.2",
    "eslint-plugin-react": "6.9.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "0.9.0",
    "find": "^0.2.7",
    "flow-bin": "0.38.0",
    "flow-coverage-report": "0.2.0",
    "flow-typed": "2.0.0",
    "gaze": "^1.1.2",
    "glob": "^7.1.1",
    "gulp": "^3.9.1",
    "happypack": "3.0.2",
    "html-webpack-plugin": "2.26.0",
    "jest": "18.1.0",
    "jquery": "^3.1.1",
    "md5": "2.2.1",
    "modernizr-loader": "1.0.1",
    "node-notifier": "4.6.1",
    "node-sass": "4.3.0",
    "now": "^3.0.0",
    "nsp": "^2.6.2",
    "postcss": "5.2.11",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.2.2",
    "postcss-simple-vars": "3.0.0",
    "react-addons-test-utils": "15.4.2",
    "regenerator-runtime": "0.10.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "semantic-ui": "^2.2.7",
    "semver": "5.3.0",
    "source-map-support": "0.4.10",
    "style-loader": "0.13.1",
    "webpack": "2.2.0",
    "webpack-bundle-analyzer": "2.2.1",
    "webpack-dashboard": "^0.2.1",
    "webpack-dev-middleware": "1.9.0",
    "webpack-hot-middleware": "2.15.0",
    "webpack-md5-hash": "0.0.5",
    "webpack-node-externals": "1.5.4",
    "yarn": "^0.19.1"
  }
}
